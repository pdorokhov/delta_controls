p1_timeON_result = (p1_timeON_hours * 100) + ((p1_timeON_minutes * 100) / 60)
p1_timeOFF_result = (p1_timeOFF_hours * 100) + ((p1_timeOFF_minutes * 100) / 60)

If DecimalTime Between p1_timeON_result p1_timeOFF_result Then
   p1_xSch = On
Else
   p1_xSch = Off
End If

If p1_disable_sch = On Then
    p1_xSch = On
End If